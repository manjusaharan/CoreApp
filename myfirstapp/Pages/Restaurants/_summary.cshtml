@model MyLib.Restaurant
<div class="panel panel-default">
    <div class="panel panel-heading">
<h3>@Model.Name</h3>
</div>
<div class="panel panel-body">
<span>@Model.Address</span>
<span>@Model.Cuisine</span>
</div>
<div class="panel panel-footer">
     <a class="btn btn-lg" asp-page = "./details" asp-route-restaurantId="@Model.Id">
    <i class="glyphicon glyphicon-zoom-in"></i></a>
     <a class="btn btn-lg" asp-page = "./edit" asp-route-restaurantId="@Model.Id">
    <i class="glyphicon glyphicon-edit"></i></a>
     <a class="btn btn-lg" asp-page = "./delete" asp-route-restaurantId="@Model.Id">
    <i class="glyphicon glyphicon-trash"></i></a>
</div>
</div>